{
  "version": 2,
  "builds": [
    {
      "src": "Admin frontend/server/index.ts",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    },
    {
      "src": "ProdigyHub-Unified/server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/ProdigyHub-Unified/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Admin frontend/server/index.ts"
    }
  ]
}
