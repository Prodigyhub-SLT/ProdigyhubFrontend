{
  "buildCommand": "cd 'Admin frontend' && npm install && npm run build",
  "outputDirectory": "Admin frontend/dist",
  "functions": {
    "ProdigyHub-Unified/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/ProdigyHub-Unified/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
